{
  "name": "danestves",
  "version": 2,
  "alias": "dev.danestves.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    },
    {
      "src": "server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/server.js" }],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "GITHUB_PERSONAL_ACCESS_TOKEN": "@danestves-gpat"
    }
  }
}