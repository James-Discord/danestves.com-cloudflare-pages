{
  "name": "danestves",
  "version": 2,
  "alias": "dev.danestves.com",
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "^/robots.txt$",
      "dest": "/utils/robots.txt",
      "headers": {
        "Content-Type": "text/plain;charset=UTF-8"
      }
    },
    {
      "src": "^/sitemap.xml$",
      "dest": "/.next/sitemap.xml",
      "headers": {
        "Content-Type": "text/xml;charset=UTF-8"
      }
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    },
    {
      "src": "utils/robots.txt",
      "use": "@now/static"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "GITHUB_PERSONAL_ACCESS_TOKEN": "@danestves-gpat",
      "GITHUB_API_URL": "@danestves-gau",
      "API_URL": "@danestves-au"
    }
  }
}