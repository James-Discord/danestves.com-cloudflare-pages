{
  "name": "danestves",
  "version": 2,
  "alias": "dev.danestves.com",
  "builds": [
    {
      "src": "ssr-server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pages/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/certificaciones",
      "dest": "/pages/certifications.js",
      "methods": [
        "GET"
      ]
    }
  ],
  "build": {
    "env": {
      "GITHUB_PERSONAL_ACCESS_TOKEN": "@danestves-gpat"
    }
  }
}