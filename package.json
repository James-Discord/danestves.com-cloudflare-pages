{
  "private": true,
  "name": "danestves.com",
  "version": "3.0.0",
  "description": "Daniel Esteves website made with remix-worker-template an all-in-one remix starter template for Cloudflare Workers by @edmundhung",
  "main": "./dist/worker.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danestves/danestves.com.git"
  },
  "keywords": [],
  "author": "Daniel Esteves <me@danestves.com> (https://danestves.com)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/danestves/danestves.com/issues"
  },
  "homepage": "https://github.com/danestves/danestves.com#readme",
  "scripts": {
    "act": "act compile-mdx -j compile --secret-file .env",
    "cleanup": "rimraf .cache ./dist ./build ./public/build ./app/styles",
    "content:watch": "node ./other/watch-content.js",
    "build": "cross-env NODE_ENV=production run-s build:* && node ./other/generate-build-info.js",
    "build:style": "postcss styles/**/*.css --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "build:worker": "cross-env NODE_ENV=production VERSION=$(git rev-parse --short HEAD) node ./scripts/build.mjs",
    "debug": "miniflare --debug --build-command \"npm run dev:worker\" --watch",
    "dev": "routes-gen -d @routes-gen/remix && pm2-dev ./other/pm2.config.js",
    "format": "prettier --write \"**/*.{js,jsx,md,mdx,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "postinstall": "remix setup cloudflare-workers",
    "prebuild": "npm run cleanup",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@headlessui/react": "^1.4.3",
    "@radix-ui/react-dropdown-menu": "^0.1.4",
    "@radix-ui/react-icons": "^1.0.3",
    "@radix-ui/react-switch": "^0.1.4",
    "@radix-ui/react-tooltip": "^0.1.6",
    "@radix-ui/react-visually-hidden": "^0.1.3",
    "@reach/portal": "^0.16.2",
    "@reach/tooltip": "^0.16.2",
    "@remix-run/cloudflare-workers": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "@stitches/react": "^1.2.6",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.2.3",
    "i18next": "^21.6.10",
    "i18next-browser-languagedetector": "^6.1.3",
    "just-merge": "^3.0.1",
    "mdx-bundler": "^8.0.1",
    "mdx-embed": "^0.0.22",
    "microtip": "^0.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-intersection-observer": "^8.33.1",
    "remix": "^1.1.3",
    "remix-i18next": "^2.1.5",
    "remix-themes": "^1.1.4",
    "remix-utils": "^2.4.0",
    "routes-gen": "^0.3.0",
    "use-consistent-value": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@cloudflare/workers-types": "^3.3.1",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@linaria/esbuild": "^3.0.0-beta.15",
    "@remix-run/dev": "^1.1.3",
    "@remix-run/eslint-config": "^1.1.3",
    "@routes-gen/remix": "^0.2.10",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.3",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.16",
    "esbuild": "^0.14.14",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "miniflare": "^2.2.0",
    "node-fetch": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.2",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.17",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.7"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,yml}": "prettier --write"
  }
}
