{
  "private": true,
  "name": "danestves.com",
  "version": "3.0.0",
  "description": "Daniel Esteves website made with remix-worker-template an all-in-one remix starter template for Cloudflare Workers by @edmundhung",
  "main": "./dist/worker.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danestves/danestves.com.git"
  },
  "keywords": [],
  "author": "Daniel Esteves <me@danestves.com> (https://danestves.com)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/danestves/danestves.com/issues"
  },
  "homepage": "https://github.com/danestves/danestves.com#readme",
  "scripts": {
    "act": "act compile-mdx -j compile --secret-file .env",
    "cleanup": "rimraf .cache ./dist ./build ./public/build ./app/styles/tailwind.css",
    "content:watch": "node ./other/watch-content.js",
    "build:remix": "remix build",
    "build:style": "postcss styles/**/*.css --base styles --dir app/styles --env production",
    "build:worker": "cross-env NODE_ENV=production VERSION=$(git rev-parse --short HEAD) node ./scripts/build.mjs",
    "build": "npm run build:style && npm run build:remix && node ./other/generate-build-info.js",
    "debug": "miniflare --debug --build-command \"npm run dev:worker\" --watch",
    "dev:remix": "remix watch",
    "dev:style": "postcss styles/**/*.css --base styles --dir app/styles --watch",
    "dev:worker": "miniflare --build-command \"node ./scripts/build.mjs\" --build-watch-path ./worker --build-watch-path ./build/index.js --no-cache --watch --open",
    "dev": "concurrently \"npm:dev:*\" \"npm:content:watch\" \"routes-gen -d @routes-gen/remix\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "postinstall": "remix setup cloudflare-workers",
    "prebuild": "npm run cleanup",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@remix-run/cloudflare-workers": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.0.0",
    "i18next": "^21.6.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "just-merge": "^3.0.1",
    "mdx-bundler": "^8.0.1",
    "mdx-embed": "^0.0.22",
    "microtip": "^0.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "remix": "^1.1.1",
    "remix-i18next": "^2.1.5",
    "remix-themes": "^1.1.4",
    "remix-utils": "^2.4.0",
    "routes-gen": "^0.3.0",
    "use-consistent-value": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@cloudflare/workers-types": "^3.3.1",
    "@remix-run/dev": "^1.1.1",
    "@remix-run/eslint-config": "^1.1.1",
    "@routes-gen/remix": "^0.2.10",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.2",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.15",
    "esbuild": "^0.14.11",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^3.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.1",
    "miniflare": "^2.2.0",
    "node-fetch": "^3.1.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.7"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,yml}": "prettier --write"
  }
}
