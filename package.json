{
  "name": "danestves.com",
  "version": "3.0.0",
  "private": true,
  "description": "Daniel Esteves website made with remix-worker-template an all-in-one remix starter template for Cloudflare Workers by @edmundhung",
  "keywords": [],
  "homepage": "https://github.com/danestves/danestves.com#readme",
  "bugs": {
    "url": "https://github.com/danestves/danestves.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danestves/danestves.com.git"
  },
  "license": "GPL-3.0-only",
  "author": "Daniel Esteves <me@danestves.com> (https://danestves.com)",
  "sideEffects": false,
  "main": "./dist/worker.js",
  "scripts": {
    "act": "act compile-mdx -j compile --secret-file .env",
    "cleanup": "rimraf .cache ./dist ./build ./public/build ./app/styles",
    "content:watch": "node ./other/watch-content.js",
    "prebuild": "npm run cleanup",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "predev": "routes-gen -d @routes-gen/remix",
    "dev": "remix build && run-p dev:*",
    "dev:remix": "remix watch",
    "dev:styles": "postcss styles/**/*.css --base styles --dir app/styles --watch",
    "dev:wrangler": "wrangler pages dev ./public --binding $(cat .env) --config wrangler.toml --kv CONTENT --kv VIEWS --kv LOCALES",
    "build:style": "postcss styles/**/*.css --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "format": "prettier --write \"**/*.{js,jsx,md,mdx,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "postinstall": "remix setup cloudflare",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@balavishnuvj/remix-seo": "^1.0.2",
    "@flyyer/flyyer": "^3.4.1",
    "@giscus/react": "^2.0.1",
    "@headlessui/react": "^1.5.0",
    "@radix-ui/react-dropdown-menu": "^0.1.6",
    "@radix-ui/react-icons": "^1.1.0",
    "@radix-ui/react-switch": "^0.1.5",
    "@radix-ui/react-tooltip": "^0.1.7",
    "@radix-ui/react-visually-hidden": "^0.1.4",
    "@reach/portal": "^0.16.2",
    "@reach/tooltip": "^0.16.2",
    "@reach/visually-hidden": "^0.16.0",
    "@remix-run/cloudflare-pages": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "@stitches/react": "^1.2.7",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "endent": "^2.1.0",
    "framer-motion": "^6.2.9",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "just-merge": "^3.0.1",
    "lodash.isequal": "^4.5.0",
    "mdx-bundler": "^9.0.0",
    "mdx-embed": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.2",
    "react-intersection-observer": "^8.33.1",
    "remix": "^1.3.4",
    "remix-i18next": "^3.0.0",
    "remix-seo": "^0.1.0",
    "remix-themes": "^1.1.4",
    "remix-utils": "^3.0.0",
    "routes-gen": "^0.3.2",
    "use-consistent-value": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@cloudflare/workers-types": "^3.4.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "@routes-gen/remix": "^0.2.10",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/tailwindcss": "^3.0.10",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.4",
    "chokidar": "^3.5.3",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
